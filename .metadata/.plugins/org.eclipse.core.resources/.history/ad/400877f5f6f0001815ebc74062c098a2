	//Busca el campo de texto llamado emailOrPhone.
	
	public static WebElement buscarBoton(WebDriver merison) {
		
		return merison.findElement(By.xpath("//a[@class='js-nav EdgeButton EdgeButton--medium EdgeButton--secondary StaticLoggedOutHomePage-buttonLogin']");
	}
	
