package com.latam.certification.models;

public class ChildPassenger {
	
	private String babyName;
	public String getBabyName() {
		return babyName;
	}
	public void setBabyName(String babyName) {
		this.babyName = babyName;
	}
	public String getBabydLastname() {
		return babydLastname;
	}
	public void setBabydLastname(String babydLastname) {
		this.babydLastname = babydLastname;
	}
	public String getBabyIdType() {
		return babyIdType;
	}
	public void setBabyIdType(String babyIdType) {
		this.babyIdType = babyIdType;
	}
	public String getBabyIdNumber() {
		return babyIdNumber;
	}
	public void setBabyIdNumber(String babyIdNumber) {
		this.babyIdNumber = babyIdNumber;
	}
	public String getBabyOrigineCountry() {
		return babyOrigineCountry;
	}
	public void setBabyOrigineCountry(String babyOrigineCountry) {
		this.babyOrigineCountry = babyOrigineCountry;
	}
	public String getBabyBirthDateDay() {
		return babyBirthDateDay;
	}
	public void setBabyBirthDateDay(String babyBirthDateDay) {
		this.babyBirthDateDay = babyBirthDateDay;
	}
	public String getBabyBirthDateMonth() {
		return babyBirthDateMonth;
	}
	public void setBabyBirthDateMonth(String babyBirthDateMonth) {
		this.babyBirthDateMonth = babyBirthDateMonth;
	}
	public String getBabyBirthDateYear() {
		return babyBirthDateYear;
	}
	public void setBabyBirthDateYear(String babyBirthDateYear) {
		this.babyBirthDateYear = babyBirthDateYear;
	}
	private String babydLastname;
	private String babyIdType;
	private String babyIdNumber;
	private String babyOrigineCountry;
	private String babyBirthDateDay;
	private String babyBirthDateMonth;
	private String babyBirthDateYear;

}
